<!-------------------SCRIPTS/JQUERY (PART ONE)------------------->

<!------------Smooth Scroll Script------------>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/smoothscroll/1.4.6/SmoothScroll.min.js"></script>
<!------------Smooth Scroll Script------------>

<!-----------Scroll To Top Script----------->
<script type="text/javascript" src="https://static.tumblr.com/ikeq9mi/DfYl6o46t/scrolltotop.min.js"></script>
<!-----------Scroll To Top Script----------->

<script>
$(document).ready(function(){
    //sticky header script
    var navOffset = $(".linkbar").offset().top;
    $(".linkbar").wrap('<div class="top-ph"></div>');
    $(".top-ph").height($(".linkbar").outerHeight());
    $(window).scroll(function() {
        var scrollPos = $(window).scrollTop();
        if (scrollPos >= navOffset) {
            $(".linkbar").addClass("sticky");
            } else {
            $(".linkbar").removeClass("sticky");
        }
    });
    //search suggestions script     
    $('.search').submit(function(event){
       var value = $('input:first').val();
       location.replace('https://{Name}.tumblr.com/tagged/' + value);
   });
   //click to posts script 
    $(".enter").click(function (){
            $('html, body').animate({
                scrollTop: $(window).height()
            }, 2000);
            return false;
    });
});
</script>
 
<!-------------------SCRIPTS/JQUERY (PART ONE)------------------->
 
</head>

<body>

<!-----------SCROLL TO TOP ----------->
<a href="javascript:;" id="scrollToTop" rel="nofollow" title="scroll to top">go to top</a>
<!-----------SCROLL TO TOP ----------->

<header class="main-header">
